@{
    ViewBag.Title = "Model3D";
}

<div id="bodyContainer" ng-app="Model3DApp">
    <section id="content">
        <div class="container">
            <div class="row">
                <div class="flip-container text-center col-md-12" ng-controller="Model3DCtrl" >
                   
                        <p ng-model="id" class="lead">{{id}}</p>
                       
                </div>
            </div>
        </div>
    </section>
</div>

@section scripts {
    @Scripts.Render("~/Scripts/angular.js")
    @Scripts.Render("~/Scripts/app/model3d-controller.js")
}